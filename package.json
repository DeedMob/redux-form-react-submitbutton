{
  "name": "redux-form-react-submitbutton",
  "version": "2.0.2",
  "description": "A submit button for redux form that dynamically changes based on the current form state",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "babel src --out-dir dist --ignore __tests__",
    "lint": "eslint src",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "prepublish": "npm run lint && npm run test && npm run clean && npm run build"
  },
  "jest": {
    "automock": false,
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DeedMob/redux-form-react-submitbutton.git"
  },
  "keywords": [
    "redux-form",
    "redux",
    "submit",
    "button"
  ],
  "author": "David Furlong <dvfurlong@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DeedMob/redux-form-react-submitbutton/issues"
  },
  "homepage": "https://github.com/DeedMob/redux-form-react-submitbutton#readme",
  "dependencies": {
    "react": "^15.4.2",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "jest": "^19.0.2",
    "jest-cli": "^19.0.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-test-renderer": "^15.4.2",
    "rimraf": "^2.6.1",
    "sinon": "^2.1.0"
  },
  "peerDependencies": {
    "redux-form": "^6.6.0"
  },
  "npmName": "redux-form-react-submitbutton",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ]
}
